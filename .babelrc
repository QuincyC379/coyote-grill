{
	"presets": [
		["env", {
			"targets": {
				"browsers": [
					"> 1%",
					"last 2 versions",
					"ie >= 9"
				]
			},
			"useBuiltIns": true,
			"debug": true
		}],
		"react",
		["es2015", {"modules": false}]
	],
	"plugins": [
		["transform-react-jsx", {"pragma": "h"}],
		"syntax-dynamic-import"
	]
}
