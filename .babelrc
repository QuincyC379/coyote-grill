{
	"presets": [
		["env", {
			"targets": {
				"browsers": [
					"> 1%",
					"last 2 versions"
				]
			},
			"useBuiltIns": true,
			"debug": true,
			"modules": false
		}],
		"react"
	],
	"plugins": [
		["transform-react-jsx", {"pragma": "h"}],
		"styled-jsx/babel",
		"syntax-dynamic-import"
	]
}
